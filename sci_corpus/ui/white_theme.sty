QGroupBox {
border: 1px solid lightgray;
border-radius: 5px;
margin-top: 0.5em;
}

QGroupBox::title {
subcontrol-origin: margin;
left: 10px;
padding: 0 3px 0 3px;
}

QDockWidget  {
    border: 2px solid gray;
}

QDockWidget::title  {
    text-align: left;
    font: bold 16px;
    background: rgb(235, 235, 235);
    border-radius: 2px;
    padding-left: 5px;
}
